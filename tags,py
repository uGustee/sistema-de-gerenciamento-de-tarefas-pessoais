def cadastrar_tag():
    id_tag = input("ID tag: ")
    nome = input("Nome: ")
    criada_em = input("Criada em: ")
    tag = {
        "id_tag": id_tag,
        "nome": nome,
        "criada_em": criada_em
    }
    return tag

def mostrar_tag(t):
    print("Tag:", t["id_tag"])
    print(" Nome:", t["nome"])
